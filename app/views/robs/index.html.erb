
<h1>List of Users</h1>
 
<table>
  <tr>
    <th>Title</th>
    <th>Content</th>
  </tr>
  <% @robs.each do |rob| %>
          <tr>
    <td><%= rob.title %></td>
    <td><%= rob.content %></td>

    <td><%= link_to 'Check the details', rob_path(rob.id) %></td>
    <td><%= link_to "Edit this user", edit_user_path(rob.id) %></td>
     <td><%= link_to 'Post a new rob', new_rob_path %></td>
  </tr>
<% end %> 
    </article>
    

</table>

<br>
    </article>
</div>
