<%= form_with(model: @user, local: true) do |form| %>

  <div class="blog_title">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  <div class="blog_title">
    <%= form.label :content %>
    <%= form.text_field :content %>
  </div>
   <h2>Create a new user</h2>

  
  <%= form.submit %>
<% end %>
